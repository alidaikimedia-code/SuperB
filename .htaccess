# SuperB Party Girl - URL Canonicalization Rules
# This file ensures consistent URL structure with trailing slashes

# Enable rewrite engine
RewriteEngine On

# Force trailing slash for directories (except root)
# This handles most duplicate URL issues

# Redirect URLs without trailing slash to URLs with trailing slash
# Exclude files (those with extensions) and root directory
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_URI} !/$
RewriteCond %{REQUEST_URI} !^/$
RewriteRule ^(.*)$ /$1/ [R=301,L]

# Redirect legacy Yoast path to WP core sitemap
RewriteRule ^sitemap_index\.xml$ /sitemap.xml [R=301,L]

